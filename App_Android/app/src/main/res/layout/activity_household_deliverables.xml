<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/activity__household_appliances"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#EFEFEF"
                tools:context="com.kjs.skywalk.app_android.Activity_HouseholdDeliverables">

    <TextView
        android:id="@+id/tv_apartment_name"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:onClick="onViewClick"
        android:drawableLeft="@drawable/nav_back"
        android:paddingLeft="15dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:layout_marginBottom="1dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
    </TextView>

    <LinearLayout
        android:id="@+id/ll_deliverables"
        android:layout_below="@+id/tv_apartment_name"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    </LinearLayout>

