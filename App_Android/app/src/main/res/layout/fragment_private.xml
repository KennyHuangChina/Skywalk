<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

        <!--android:background="#E5E5E5"-->
        <RelativeLayout
            android:id="@+id/rl_titlebar"
            android:layout_width="match_parent"
            android:layout_height="95dp"
            android:background="#00AE63"
            android:visibility="visible">

            <ImageView
                android:id="@+id/iv_head_portrait"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="33dp"
                android:layout_marginTop="29dp"  />
            <RelativeLayout
                android:id="@+id/rl_user_not_login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/iv_head_portrait"
                android:visibility="visible">
                <TextView
                    android:id="@+id/tv_clicktologin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="onFragmentPrivateClickResponse"
                    android:layout_marginTop="49dp"
                    android:layout_marginStart="10dp"
                    android:textColor="#000000"
                    android:text="点击登录"
                    android:textSize="17sp"/>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/ll_user_login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="42dp"
            android:layout_marginStart="11dp"
            android:orientation="vertical"
            android:layout_toEndOf="@+id/iv_head_portrait"
            android:visibility="gone">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <EditText
                    android:id="@+id/et_user_name"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:gravity="left|center_vertical"
                    android:ems="11"
                    android:background="@null"
                    android:textSize="12sp"
                    android:textColor="#000000"
                    android:inputType="text" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/edit1"/>
            </LinearLayout>
            <TextView
                android:id="@+id/tv_profile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:paddingStart="3dp"
                android:drawableRight="@drawable/right4_n"
                android:drawablePadding="5dp"
                android:textColor="#ffffff"
                android:text="个人信息"
                android:textSize="12sp"/>
        </LinearLayout>

            <!--android:src="@drawable/mesage3_n"-->
        <ImageView
            android:id="@+id/iv_message"
            android:contentDescription="@string/app_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="32dp"
            android:layout_marginTop="19dp"
            android:src="@drawable/message4"/>

            <!--android:src="@drawable/setting_n"-->
        <ImageView
            android:id="@+id/iv_setting"
            android:contentDescription="@string/app_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toStartOf="@+id/iv_message"
            android:layout_marginEnd="11dp"
            android:layout_marginTop="19dp"
            android:src="@drawable/setting1"/>

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="74dp"
        android:orientation="horizontal"
        android:background="@drawable/background"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/ll_watchlist_count"
            android:onClick="onFragmentPrivateClickResponse"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:id="@+id/tv_watchlist_count"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:duplicateParentState="true"
                android:text="0"
                android:textSize="10sp"
                android:gravity="center"
                android:textColor="@drawable/itemcount1_text_selector"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="我的关注"
                android:textSize="12sp"
                android:gravity="center"
                android:textColor="#333333"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_browsing_history"
            android:onClick="onFragmentPrivateClickResponse"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:id="@+id/tv_browsing_history"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:duplicateParentState="true"
                android:text="0"
                android:textSize="10sp"
                android:gravity="center"
                android:textColor="@drawable/itemcount1_text_selector"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="浏览记录"
                android:textSize="12sp"
                android:gravity="center"
                android:textColor="#333333"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_see_count"
            android:onClick="onFragmentPrivateClickResponse"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:duplicateParentState="true"
                android:text="0"
                android:textSize="10sp"
                android:gravity="center"
                android:textColor="@drawable/itemcount1_text_selector"/>
            <TextView
                android:id="@+id/tv_see_count"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="看房记录"
                android:textSize="12sp"
                android:gravity="center"
                android:textColor="#333333"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_appointment_count"
            android:onClick="onFragmentPrivateClickResponse"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:orientation="vertical">
            <TextView
                android:id="@+id/tv_appointment_count"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:duplicateParentState="true"
                android:text="0"
                android:textSize="10sp"
                android:gravity="center"
                android:textColor="@drawable/itemcount1_text_selector"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="我的预约"
                android:textSize="12sp"
                android:gravity="center"
                android:textColor="#333333"/>
        </LinearLayout>
    </LinearLayout>

    <!-- 我的交易    start -->
    <include layout="@layout/seperator_horizontal"
        android:layout_width="match_parent"
        android:layout_height="7dp"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="20dp"
            android:gravity="center_vertical"
            android:text="我的交易"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="16sp"/>
        <TextView
            android:id="@+id/tv_rental_sales"
            android:onClick="onFragmentPrivateClickResponse"
            android:layout_width="62dp"
            android:layout_height="26dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="20dp"
            android:gravity="center"
            android:text="租售委托"
            style="@style/btnstyle_major"
            android:textSize="12sp"/>
    </RelativeLayout>

    <include layout="@layout/seperator_horizontal"/>
    
    <RelativeLayout
        android:id="@+id/rl_transaction"
        android:onClick="onFragmentPrivateClickResponse"
        android:layout_width="match_parent"
        android:layout_height="36dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="30dp"
            android:gravity="center_vertical"
            android:text="我的交易"
            android:textColor="#333333"
            android:textSize="13sp"/>
        <TextView
            android:id="@+id/tv_transaction"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:gravity="center_vertical"
            android:text="0"
            android:drawableRight="@drawable/right1_n"
            android:drawablePadding="8dp"
            android:textColor="@drawable/itemcount_text_selector"
            android:textSize="10sp"/>
    </RelativeLayout>

    <include layout="@layout/seperator_horizontal"/>

    <RelativeLayout
        android:id="@+id/rl_rent_out"
        android:onClick="onFragmentPrivateClickResponse"
        android:layout_width="match_parent"
        android:layout_height="36dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="30dp"
            android:gravity="center_vertical"
            android:text="我已租出"
            android:textColor="#333333"
            android:textSize="13sp"/>
        <TextView
            android:id="@+id/tv_rent_out"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:gravity="center_vertical"
            android:text="0"
            android:drawableRight="@drawable/right1_n"
            android:drawablePadding="8dp"
            android:textColor="@drawable/itemcount_text_selector"
            android:textSize="10sp"/>
    </RelativeLayout>

    <include layout="@layout/seperator_horizontal"/>

    <RelativeLayout
        android:id="@+id/rl_house_rented"
        android:onClick="onFragmentPrivateClickResponse"
        android:layout_width="match_parent"
        android:layout_height="36dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="30dp"
            android:gravity="center_vertical"
            android:text="我已租到"
            android:textColor="#333333"
            android:textSize="13sp"/>
        <TextView
            android:id="@+id/tv_house_rented"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:gravity="center_vertical"
            android:text="0"
            android:drawableRight="@drawable/right1_n"
            android:drawablePadding="8dp"
            android:textColor="@drawable/itemcount_text_selector"
            android:textSize="10sp"/>
    </RelativeLayout>

    <include layout="@layout/seperator_horizontal"/>

    <RelativeLayout
        android:id="@+id/rl_lease"
        android:onClick="onFragmentPrivateClickResponse"
        android:layout_width="match_parent"
        android:layout_height="36dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="30dp"
            android:gravity="center_vertical"
            android:text="我的租约"
            android:textColor="#333333"
            android:textSize="13sp"/>
        <TextView
            android:id="@+id/tv_lease"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:gravity="center_vertical"
            android:text="0"
            android:drawableRight="@drawable/right1_n"
            android:drawablePadding="8dp"
            android:textColor="@drawable/itemcount_text_selector"
            android:textSize="10sp"/>
    </RelativeLayout>
    <!-- 我的交易    end -->

    <!-- 我代理的房源    start -->
    <include layout="@layout/seperator_horizontal"
        android:layout_width="match_parent"
        android:layout_height="7dp"/>

    <RelativeLayout
        android:id="@+id/rl_agency_houses"
        android:onClick="onFragmentPrivateClickResponse"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="20dp"
            android:gravity="center_vertical"
            android:text="我代理的房源"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="16sp"/>
        <TextView
            android:id="@+id/tv_agency_houses"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:gravity="center_vertical"
            android:text="所有房源 ( 0 )"
            android:drawableRight="@drawable/right3_n"
            android:drawablePadding="8dp"
            android:textColor="#666666"
            android:textSize="12sp"/>
    </RelativeLayout>

    <include layout="@layout/seperator_horizontal"/>

    <!--<RelativeLayout-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="36dp">-->
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="match_parent"-->
            <!--android:layout_marginStart="30dp"-->
            <!--android:gravity="center_vertical"-->
            <!--android:text="我已代理"-->
            <!--android:textColor="#333333"-->
            <!--android:textSize="13sp"/>-->
        <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="match_parent"-->
            <!--android:layout_alignParentEnd="true"-->
            <!--android:layout_marginEnd="29dp"-->
            <!--android:gravity="center_vertical"-->
            <!--android:text="0"-->
            <!--android:drawableRight="@drawable/right1_n"-->
            <!--android:drawablePadding="8dp"-->
            <!--android:textColor="#B2B2B2"-->
            <!--android:textSize="10sp"/>-->
    <!--</RelativeLayout>-->

    <RelativeLayout
        android:id="@+id/rl_to_rent"
        android:onClick="onFragmentPrivateClickResponse"
        android:layout_width="match_parent"
        android:layout_height="36dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="30dp"
            android:gravity="center_vertical"
            android:text="待租中"
            android:textColor="#333333"
            android:textSize="13sp"/>
        <TextView
            android:id="@+id/tv_to_rent"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:gravity="center_vertical"
            android:text="0"
            android:drawableRight="@drawable/right1_n"
            android:drawablePadding="8dp"
            android:textColor="@drawable/itemcount_text_selector"
            android:textSize="10sp"/>
    </RelativeLayout>

    <include layout="@layout/seperator_horizontal"/>

    <RelativeLayout
        android:id="@+id/rl_rented"
        android:onClick="onFragmentPrivateClickResponse"
        android:layout_width="match_parent"
        android:layout_height="36dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="30dp"
            android:gravity="center_vertical"
            android:text="出租中"
            android:textColor="#333333"
            android:textSize="13sp"/>
        <TextView
            android:id="@+id/tv_rented"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:gravity="center_vertical"
            android:text="0"
            android:drawableRight="@drawable/right1_n"
            android:drawablePadding="8dp"
            android:textColor="@drawable/itemcount_text_selector"
            android:textSize="10sp"/>
    </RelativeLayout>

    <include layout="@layout/seperator_horizontal"/>

    <RelativeLayout
        android:id="@+id/rl_to_sale"
        android:onClick="onFragmentPrivateClickResponse"
        android:layout_width="match_parent"
        android:layout_height="36dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="30dp"
            android:gravity="center_vertical"
            android:text="待售中"
            android:textColor="#333333"
            android:textSize="13sp"/>
        <TextView
            android:id="@+id/tv_to_sale"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:gravity="center_vertical"
            android:text="0"
            android:drawableRight="@drawable/right1_n"
            android:drawablePadding="8dp"
            android:textColor="@drawable/itemcount_text_selector"
            android:textSize="10sp"/>
    </RelativeLayout>

    <include layout="@layout/seperator_horizontal"/>

    <RelativeLayout
        android:id="@+id/rl_month_turnoff"
        android:onClick="onFragmentPrivateClickResponse"
        android:layout_width="match_parent"
        android:layout_height="36dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="30dp"
            android:gravity="center_vertical"
            android:text="本月成交"
            android:textColor="#333333"
            android:textSize="13sp"/>
        <TextView
            android:id="@+id/tv_month_turnoff"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:gravity="center_vertical"
            android:text="0"
            android:drawableRight="@drawable/right1_n"
            android:drawablePadding="8dp"
            android:textColor="@drawable/itemcount_text_selector"
            android:textSize="10sp"/>
    </RelativeLayout>

    <include layout="@layout/seperator_horizontal"/>

    <RelativeLayout
        android:id="@+id/rl_to_approve"
        android:onClick="onFragmentPrivateClickResponse"
        android:layout_width="match_parent"
        android:layout_height="36dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="30dp"
            android:gravity="center_vertical"
            android:text="待审核"
            android:textColor="#333333"
            android:textSize="13sp"/>
        <TextView
            android:id="@+id/tv_to_approve"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:duplicateParentState="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="29dp"
            android:gravity="center_vertical"
            android:text="0"
            android:drawableRight="@drawable/right1_n"
            android:drawablePadding="8dp"
            android:textColor="@drawable/itemcount_text_selector"
            android:textSize="10sp"/>
    </RelativeLayout>
    <!-- 我代理的房源    end -->

    <!-- 产品服务    start -->
    <include layout="@layout/seperator_horizontal"
        android:layout_width="match_parent"
        android:layout_height="7dp"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:visibility="visible">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginStart="20dp"
            android:gravity="center_vertical"
            android:text="产品服务"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="16sp"/>
    </RelativeLayout>

    <include layout="@layout/seperator_horizontal"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="80dp">
        <TextView
            android:id="@+id/tv_feedback"
            android:text="意见反馈"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="35dp"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:drawableTop="@drawable/feedback"
            android:drawablePadding="5dp"
            android:textColor="#B2B2B2"
            android:textSize="10sp"
            android:onClick="onFragmentPrivateClickResponse" />
        <TextView
            android:id="@+id/tv_feedback_recorder"
            android:text="反馈记录"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="35dp"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:drawableTop="@drawable/feedback_recorder"
            android:drawablePadding="5dp"
            android:textColor="#B2B2B2"
            android:textSize="10sp"
            android:onClick="onFragmentPrivateClickResponse" />
    </LinearLayout>

    <!-- 产品服务    end -->

</LinearLayout>
</ScrollView>