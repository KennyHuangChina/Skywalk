<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.kjs.skywalk.app_android.Activity_fangyuan_zhaopian">

    <TextView
        android:id="@+id/tv_info_title"
        android:onClick="onViewClick"
        style="@style/textStyle_page_title"
        android:text="房源照片"/>

    <include layout="@layout/seperator_horizontal"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/fl_huxing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </FrameLayout>

            <include layout="@layout/seperator_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>

            <FrameLayout
                android:id="@+id/fl_fangjianjiegou"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </FrameLayout>

            <include layout="@layout/seperator_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>

            <FrameLayout
                android:id="@+id/fl_jiajuyongpin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </FrameLayout>

            <include layout="@layout/seperator_horizontal"
                android:layout_width="match_parent"
                android:layout_height="5dp"/>

            <FrameLayout
                android:id="@+id/fl_dianqi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            </FrameLayout>

        </LinearLayout>
    </ScrollView>

</LinearLayout>
