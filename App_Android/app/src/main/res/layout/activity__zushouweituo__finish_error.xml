<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_rentals_expiration"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:orientation="vertical"

    tools:context="com.kjs.skywalk.app_android.Activity_rentals_expiration">

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:background="@color/colorBackgroundCommon">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/textViewBack"
                android:onClick="onClickResponse"
                android:layout_width="0dp"
                android:layout_height="45dp"
                style="@style/btnstyle_back"
                android:layout_centerInParent="true"
                android:layout_margin="15dp"
                android:text="返回"
                android:layout_weight="9"/>
            <TextView
                android:id="@+id/textViewCommitAgain"
                android:onClick="onClickResponse"
                android:layout_width="0dp"
                android:layout_height="45dp"
                style="@style/btnstyle_again"
                android:layout_centerInParent="true"
                android:layout_margin="15dp"
                android:text="重新提交"
                android:layout_weight="11"/>
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:id="@+id/linearLayout"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <include layout="@layout/layout_title_bar_with_close_button" />

        <include layout="@layout/seperator_horizontal"/>

        <include layout="@layout/layout_zushou_progress5_error"/>

        <include layout="@layout/seperator_horizontal"
            android:layout_width="match_parent"
            android:layout_height="7dp"/>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"  >

                <TextView
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="21dp"
                    android:layout_marginBottom="8dp"
                    android:gravity="center"
                    android:textSize="14sp"
                    android:textColor="@color/colorTextError"
                    android:drawableLeft="@drawable/warning_icon"
                    android:drawablePadding="5dp"
                    android:text="委托代理失败"/>

                <TextView
                    android:id="@+id/textViewHouseLocation"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="21dp"
                    android:gravity="center"
                    android:textSize="14sp"
                    android:textColor="@color/colorTextNormal"
                    android:text="世贸蝶湖湾175栋2108室"/>

                <include layout="@layout/seperator_horizontal"/>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="60dp">
                    <TextView
                        android:id="@+id/textViewReason"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:gravity="center"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="16dp"
                        android:text="您的费用没有支付"
                        android:textColor="@color/colorTextRed" />
                </RelativeLayout>

                <include layout="@layout/seperator_horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="8dp"/>

                <LinearLayout
                    android:id="@+id/uploadContainer"
                    android:layout_width="wrap_content"
                    android:layout_height="83dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:orientation="horizontal"
                    android:onClick="onClickResponse"
                    android:layout_gravity="center">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="5dp"
                        android:src="@drawable/service_icon"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:orientation="vertical"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="5dp" >                      <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="13sp"
                            android:textColor="@color/colorTextError"
                            android:text="如有疑问请联系客服"/>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/colorTextTel"
                            android:textSize="19sp"
                            android:text="400-888-8888"/>
                    </LinearLayout>

                </LinearLayout>

                <include layout="@layout/seperator_horizontal"/>

            </LinearLayout>
        </ScrollView>


    </LinearLayout>

</RelativeLayout>