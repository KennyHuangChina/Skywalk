<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--今日推荐-->
    <style name="TextView_section_title" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#394142</item>
    </style>

    <!--查看全部1234套-->
    <style name="TextView_section_title_all" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableRight">@drawable/arrow_right</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#979aa1</item>
    </style>

    <!--证大大拇指广场-->
    <style name="TextView_apartment_name" parent="@android:style/Widget.TextView">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#242224</item>
    </style>

    <!--姑苏白杨湾街道 / 3室2厅 / 88 m²-->
    <style name="TextView_apartment_description" parent="@android:style/Widget.TextView">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#939499</item>
    </style>

    <!--随时看房-->
    <style name="TextView_cg1" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">65dp</item>
        <item name="android:layout_height">39dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/homepage_cg1</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#27bf82</item>
    </style>

    <!--满2年-->
    <style name="TextView_cg2" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">55dp</item>
        <item name="android:layout_height">39dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/homepage_cg2</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#5aaaaf</item>
    </style>

    <!--满五唯一-->
    <style name="tv_apartment_cg1" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/rounded_rect_background1</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#5aaaaf</item>
    </style>

    <!--上门实勘-->
    <style name="tv_apartment_cg2" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/rounded_rect_background2</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#4CA3FF</item>
    </style>

    <!--靠近地铁-->
    <style name="tv_apartment_cg3" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/rounded_rect_background3</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#FF3F29</item>
    </style>

    <!--电梯房-->
    <style name="tv_apartment_cg4" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/rounded_rect_background4</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#8D7DEF</item>
    </style>

    <!--无贷款-->
    <style name="tv_apartment_cg5" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/rounded_rect_background5</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#E90132</item>
    </style>

    <!--高级搜索->筛选->条件-->
    <style name="text_view_search_condition_filter_item" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_search_condition_filter</item>
        <item name="android:textSize">@dimen/text_size_search_condition_filter_item</item>
        <item name="android:textColor">@drawable/selector_search_condition_filter_item_text_color
        </item>
        <item name="android:onClick">onSearchConditionFilterItemClicked</item>
        <item name="android:layout_weight">1</item>
    </style>

    <!--高级搜索->房型-->
    <style name="text_view_search_condition_house_type_item" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:paddingLeft">17dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/text_size_search_condition_house_type_item</item>
        <item name="android:textColor">
            @drawable/selector_search_condition_house_type_item_text_color
        </item>
        <item name="android:onClick">onSearchConditionHouseTypeItemClicked</item>
    </style>

    <!--高级搜索->价格-->
    <style name="text_view_search_condition_price_item" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:paddingLeft">17dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/text_size_search_condition_price_item</item>
        <item name="android:textColor">@drawable/selector_search_condition_price_item_text_color
        </item>
        <item name="android:onClick">onSearchConditionPriceItemClicked</item>
    </style>


    <!--150万-->
    <style name="TextView_cost" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#ff3d19</item>
    </style>

    <!--25437元/平-->
    <style name="TextView_unitcost" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#939499</item>
    </style>

    <!--世贸蝶湖湾175栋2202室-->
    <style name="textstyle_large">
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">#242224</item>
    </style>

    <style name="textstyle_small">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#242224</item>
    </style>

    <!--出租进度-签约-->
    <style name="textStyle_page_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:onClick">onViewClick</item>
        <item name="android:drawableLeft">@drawable/nav_back</item>
        <item name="android:textColor">#242224</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">@dimen/page_left_margin</item>
        <item name="android:paddingRight">@dimen/page_right_margin</item>
    </style>

    <!--世茂蝶湖湾 175栋3212室-->
    <style name="textStyle_page_apartmentName">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#000000</item>
    </style>

    <!--业主信息-->
    <style name="textStyle_page_blockTitle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#000000</item>
    </style>

    <!--出租人-->
    <style name="textStyle_page_blockKey">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:textColor">@color/colorTextTint</item>
        <item name="android:textSize">14sp</item>
    </style>

    <!--荼蘼之恋-->
    <style name="textStyle_page_blockValue">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">14sp</item>
    </style>


    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
</resources>
